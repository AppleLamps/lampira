{
    "version": 2,
    "framework": null,
    "buildCommand": null,
    "outputDirectory": null,
    "rewrites": [
        {
            "source": "/api/chat",
            "destination": "/api/chat.js"
        },
        {
            "source": "/api/models",
            "destination": "/api/models.js"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}